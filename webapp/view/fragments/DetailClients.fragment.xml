<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"    
    xmlns:r="sap.suite.ui.microchart"
	xmlns:l="sap.ui.layout" 
	xmlns:m="sap.m"	
    xmlns:ux="sap.uxap"
>
    <ux:ObjectPageLayout id="ObjectPageLayoutClient" showTitleInHeaderContent="true">
		<ux:headerTitle>
			<ux:ObjectPageHeader id="headerClient"
							  objectTitle="Datos de Cabecera"
							  showTitleSelector="false"
							  showMarkers="false"
							  markFavorite="true"
							  markFlagged="true"
							  markChanges="false"
							  objectSubtitle="prueba"							  
							  isObjectTitleAlwaysVisible="false"
							  isObjectSubtitleAlwaysVisible="false"
							  showPlaceholder="false">	
							  	<ux:breadcrumbs>					
				</ux:breadcrumbs>		
			</ux:ObjectPageHeader>
		</ux:headerTitle>
		<ux:headerContent>
					
		</ux:headerContent>
		<ux:sections>
			<ux:ObjectPageSection id="pageSectionPosicionesClient" title="Posiciones">
				<ux:subSections>
					<ux:ObjectPageSubSection id="pageSubsectionPosicionesClient">
						<ux:blocks>
						<l:VerticalLayout id="verticalLayoutTablaBotones2">
							
                        <l:HorizontalLayout id="horizontalLayoutBotones2">				
                        <Button id="downloadButton2" text="Descargar orden de pago"/>
                        <Button id="downloadButtonRet2"  class="sapUiSmallMarginBegin" text="Este pago no tiene retenciones" enabled="false"/>
                        </l:HorizontalLayout >				
                        </l:VerticalLayout>
						</ux:blocks>
					
					</ux:ObjectPageSubSection>
					
				</ux:subSections>
			</ux:ObjectPageSection>  
			<ux:ObjectPageSection id="pageSection" title="Documentos cancelados">
				<ux:subSections>
					<ux:ObjectPageSubSection id="pageSubsection">
						<ux:blocks>
						
					
						</ux:blocks>
					</ux:ObjectPageSubSection>
					
				</ux:subSections>
			</ux:ObjectPageSection>        
		</ux:sections>
	</ux:ObjectPageLayout>
</core:FragmentDefinition>